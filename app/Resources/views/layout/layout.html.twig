                        <li><a href="{{ path('indicasousprodser_index') }}" class= 'btn1'>Indicateur sous produit|service</a></li>
                    </ul>
                    <ul class="nav navbar-nav navbar-right">
                        <li><a href="./"> Recherche</a></li>
                        <li>
                            {% if is_granted('IS_AUTHENTICATED_FULLY') %}
                               <a href="{{path('fos_user_security_logout') }}" class=""> Se déconnecter </a>
                              
                            {% else %}
                                <a href="{{path('fos_user_security_login') }}" class=""> Se connecter </a>
                            {% endif %}  
                        </li>
                    </ul>
                </div><!--/.nav-collapse -->
            </div><!--/.container-fluid -->
        </nav>
        
        <img src="{{ absolute_url(asset('img/dci.PNG')) }}" width="100%" id="imgBarriere"> <br><br>
        
        {% block body %}{% endblock %}
    
    {% block javascripts %}{% endblock %}
   
    <footer  class="piedPage">
